# Discord Bot Token
TOKEN=<discord bot token>

# Discord Bot Client ID
CLIENTID=<discord bot id>

# Discord Guild ID
GUILDID=<discord guild id>

# Cloudflare D1 Database REST API Endpoint (legacy, not used with official SDK)
D1_ENDPOINT=<d1 endpoint>

# Cloudflare D1 Database API Auth Token (Bearer ... or just token, or use CLOUDFLARE_API_TOKEN below)
D1_AUTH=<d1 auth token>

# Cloudflare API Token for official SDK (preferred)
CLOUDFLARE_API_TOKEN=<cloudflare api token>

# Cloudflare D1 Database ID (required for official SDK)
D1_DATABASE_ID=<d1 database id>

# Cloudflare Account ID (required for official SDK)
CLOUDFLARE_ACCOUNT_ID=<cloudflare account id>
